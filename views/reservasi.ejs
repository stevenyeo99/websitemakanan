<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Bootstrap demo</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
      crossorigin="anonymous"
    />

    <link rel="stylesheet" href="./reservasi.css" type="text/css" />
  </head>
  <body>
    <!--header reservation-->
    <div class="header">
      <h1>RESERVASI MEJA</h1>
    </div>
    <!--header reservation end-->

    <!--reservation-->
    <div class="reservation">
      <p style="padding-left: 55px">
        Silahkan Memilih Meja yang Akan Digunakan
      </p>

      <!--line 1-->
      <div class="container text-center">
        <div class="row justify-content-evenly column">
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">1</div>
            </a>
          </div>
          <div class="col-md ordered">2</div>
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">3</div></a
            >
          </div>
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">4</div></a
            >
          </div>
        </div>
      </div>
      <!--line 1 end-->

      <!--line 2-->
      <div class="container text-center">
        <div class="row justify-content-evenly column">
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">5</div></a
            >
          </div>
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">6</div></a
            >
          </div>
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">7</div></a
            >
          </div>
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">8</div></a
            >
          </div>
        </div>
      </div>
      <!--line 2 end-->

      <!--line 3-->
      <div class="container text-center">
        <div class="row justify-content-evenly column">
          <div class="col-md regular">
            <a href="/transfer" style="text-decoration: none">
              <div class="one">9</div></a
            >
          </div>
          <div class="col-md vip">10</div>
          <div class="col-md vip">11</div>
          <div class="col-md birthday">12</div>
        </div>
      </div>
      <!--line 3 end-->
      <div class="buttonGoto">
        <div>
          <a href="/panduan">
            <input
              type="button"
              value="Panduan Reservasi"
              style="
                border-width: 0;
                width: 170px;
                height: 40px;
                border-radius: 15px;
                margin-right: 10px;
              "
            />
          </a>
        </div>
        <div>
          <a href="/menu">
            <input
              type="button"
              value="Kembali"
              style="
                border-width: 0;
                width: 170px;
                height: 40px;
                border-radius: 15px;
                margin-left: 10px;
              "
            />
          </a>
        </div>
      </div>
    </div>
    <!--reservation end-->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
